<%= form_for :session, url: login_path do |f| %> <!-- The path form will send requests POST sessions#create -->
  <div>
    <%= f.label :email %>
    <%= f.text_field :email %>
  </div>

  <div>
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>


  <div>
    <%= f.submit 'Login' %>
  </div>
<% end %>